(library
 (name llvm)
 (public_name llvm)
 (flags
  (:standard -w -50 -w -37 -w -34 -w -32))
 (foreign_stubs
  (language c)
  (names llvm_ocaml)
  (flags
   -w
   -I%{env:LLVM_INCLUDE=/usr/include/llvm-15}
   -I%{env:LLVM_C_INCLUDE=/usr/include/llvm-c-15}
   :standard))
 (c_library_flags -L%{env:LLVM_LIBDIR=/usr/lib/llvm-15/lib} -lLLVM))
